{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "UseOnlyInMemoryDatabase": "false",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Initial Catalog=NetApiCleanTemplate.Database;Integrated Security=true;",
    "IdentityConnection": "Server=(localdb)\\mssqllocaldb;Initial Catalog=NetApiCleanTemplate.Identity;Integrated Security=true;"
  },

  "Multitenancy": {
    "Enabled": "true",
    "ConnectionStrings": {
      "TenantA": "Server=(localdb)\\mssqllocaldb;Initial Catalog=NetApiCleanTemplate.Database.TenantA;Integrated Security=true;",
      "TenantB": "Server=(localdb)\\mssqllocaldb;Initial Catalog=NetApiCleanTemplate.Database.TenantB;Integrated Security=true;",
      "TenantC": "Server=(localdb)\\mssqllocaldb;Initial Catalog=NetApiCleanTemplate.Database.TenantC;Integrated Security=true;"
    }
  }
}

